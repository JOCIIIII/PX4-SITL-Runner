#!/bin/bash
# CONFIGURATION FILE FOR docker-compose.wsl.yml
## USE ONLY FOR WSL ENVIRONMENT WITH WSLg AVAILABILITY (WINDOWS 11)

# -------- [GENERIC DISPLAY SETTINGS] ---------

## ------- GENERATE IT BY SHELL SCRIPT --------
## ---------- DO NOT MODIFY MANUALLY ----------
DISPLAY=:0
WAYLAND_DISPLAY=wayland-0
## --------------------------------------------

## --------------- STATIC VALUES --------------
## --------------- DO NOT MODIFY --------------
## ---- UNLESS YOU KNOW WHAT YOU ARE DOING ----
X11_SOCKET_DIR=/tmp/.X11-unix
NVIDIA_DRIVER_CAPABILITIES=all
## --------------------------------------------


# ------- [CONTAINER SERVICE SETTINGS] --------

## --------------- STATIC VALUES --------------
## --------------- DO NOT MODIFY --------------
## ---- UNLESS YOU KNOW WHAT YOU ARE DOING ----
CONTAINER_NET_NAME=sitl-net
CONTAINER_NET_SUBNET=172.21.0.0/24
CONTAINER_NET_GATEWAY=172.21.0.1

CONTAINER_PX4_ADDR=172.21.0.5
CONTAINER_SIM_ADDR=172.21.0.6
CONTAINER_AIRSIM_ADDR=172.21.0.7

CONTAINER_QGC_ADDR=172.21.0.8

CONTAINER_ROS2_UXRCE_ADDR=172.21.0.9
CONTAINER_ROS2_PATH_PLANNING_ADDR=172.21.0.10
CONTAINER_ROS2_PATH_FOLLOWING_ADDR=172.21.0.11
CONTAINER_ROS2_COLLISION_AVOIDANCE_ADDR=172.21.0.12
CONTAINER_ROS2_CONTROLLER_ADDR=172.21.0.13
ROS_DOMAIN_ID=0
## --------------------------------------------


## --------------- COMMON VALUES --------------
## --------- SHARED AMONG PX4 AND SIM ---------
SITL_AIRFRAME_ID=10016
SITL_AIRFRAME=iris
SITL_TYPE=gazebo-classic

PX4_HOME_LAT=37.384215
PX4_HOME_LON=126.656648
PX4_HOME_ALT=0.0

SITL_WORLD=empty
## --------------------------------------------



## ----- PX4- CONTAINER SERVICE ----
CONTAINER_PX4_NAME=sitl-px4-001
CONTAINER_PX4_HOSTNAME=${CONTAINER_PX4_NAME}
CONTAINER_PX4_IMAGE=jociiiii/px4:dev-1.14.0-focal-0.0.1
UXRCE_DDS_CFG=1000
PX4_UXRCE_DDS_NS=

PX4_DEBUG_MODE=0
PX4_EXPORT_ENV=1
## --------------------------------------------


## ----- Gazebo Classic CONTAINER SERVICE -----
CONTAINER_SIM_NAME=sitl-sim
CONTAINER_SIM_HOSTNAME=${CONTAINER_SIM_NAME}
CONTAINER_SIM_IMAGE=kestr3l/gazebo:classic-11

# PX4_GZ_MODELS=/home/user/PX4-Autopilot/Tools/simulation/gz/models
# PX4_GZ_WORLDS=/home/user/PX4-Autopilot/Tools/simulation/gz/worlds
# PX4_GZ_PLUGINS=
GAZEBO_USER_MODEL_PATH=/home/user/gazebo/models
GAZEBO_USER_RESOURCE_PATH=/home/user/gazebo/resources

GAZEBO_HEADLESS=0
SIM_DEBUG_MODE=0
SIM_EXPORT_ENV=1
## --------------------------------------------


## ----- AirSIM CONTAINER SERVICE -----
CONTAINER_AIRSIM_NAME=sitl-airsim
CONTAINER_AIRSIM_HOSTNAME=${CONTAINER_AIRSIM_NAME}
CONTAINER_AIRSIM_IMAGE=jociiiii/airsim:dev-1.8.3

PULSEAUDIO_DIR=/run/user/1000/pulse
UNREAL_BINARY_DIR=<YOUR_UNREAL_BINARY_DIR>
PIXELSTREAM=0
REBUILD_AIRSIM=0
AIRSIM_DEBUG_MODE=0
AIRSIM_EXPORT_ENV=1
## --------------------------------------------


## ----- QGroundControl CONTAINER SERVICE -----
CONTAINER_QGC_NAME=sitl-qgc
CONTAINER_QGC_HOSTNAME=${CONTAINER_QGC_NAME}
CONTAINER_QGC_IMAGE=kestr3l/qgc-app:4.2.9

QGC_HEADLESS=0
## --------------------------------------------


## ---- ROS2 uXRCE AGENT CONTAINER SERVICE ----
CONTAINER_ROS2_UXRCE_NAME=sitl-ros2-uxrce
CONTAINER_ROS2_UXRCE_HOSTNAME=${CONTAINER_ROS2_UXRCE_NAME}
CONTAINER_ROS2_UXRCE_IMAGE=kestr3l/ros2:uxrce-humble

ROS2_XRCE_DEBUG_MODE=0
ROS2_XRCE_SIM_EXPORT_ENV=1
## --------------------------------------------


## ---- ROS2 PATH PLANNING CONTAINER SERVICE ----
CONTAINER_ROS2_PATH_PLANNING_NAME=sitl-ros2-pp
CONTAINER_ROS2_PATH_PLANNING_HOSTNAME=${CONTAINER_ROS2_PATH_PLANNING_NAME}
CONTAINER_ROS2_PATH_PLANNING_IMAGE=jociiiii/ros2:dev-humble-cu1180d-pp-v0.9
PATH_PLANNING_SOURCE_CODE_DIR=<YOUR_DIR_TO_PATH_PLANNING_SOURCE_CODE>

ROS2_PATH_PLANNING_DEBUG_MODE=0
ROS2_PATH_PLANNING_REBUILD_MODE=0
ROS2_PATH_PLANNING_SIM_EXPORT_ENV=1
ROS2_PATH_PLANNING_RUN_MODE=1
## --------------------------------------------

## ---- ROS2 PATH FOLLOWING CONTAINER SERVICE ----
CONTAINER_ROS2_PATH_FOLLOWING_NAME=sitl-ros2-pf
CONTAINER_ROS2_PATH_FOLLOWING_HOSTNAME=${CONTAINER_ROS2_PATH_FOLLOWING_NAME}
CONTAINER_ROS2_PATH_FOLLOWING_IMAGE=jociiiii/ros2:dev-humble-cu1180d-pf-v0.9
PATH_FOLLOWING_SOURCE_CODE_DIR=<YOUR_DIR_TO_PATH_FOLLOWING_SOURCE_CODE>

ROS2_PATH_FOLLOWING_DEBUG_MODE=0
ROS2_PATH_FOLLOWING_REBUILD_MODE=0
ROS2_PATH_FOLLOWING_SIM_EXPORT_ENV=1
## --------------------------------------------

## ---- ROS2 COLLISION AVOIDANCE CONTAINER SERVICE ----
CONTAINER_ROS2_COLLISION_AVOIDANCE_NAME=sitl-ros2-ca
CONTAINER_ROS2_COLLISION_AVOIDANCE_HOSTNAME=${CONTAINER_ROS2_COLLISION_AVOIDANCE_NAME}
CONTAINER_ROS2_COLLISION_AVOIDANCE_IMAGE=jociiiii/ros2:dev-galactic-tensorrt2212-ca-v1.0
COLLISION_AVOIDANCE_SOURCE_CODE_DIR=<YOUR_DIR_TO_COLLISION_AVOIDANCE_SOURCE_CODE>

ROS2_COLLISION_AVOIDANCE_DEBUG_MODE=0
ROS2_COLLISION_AVOIDANCE_REBUILD_MODE=0
ROS2_COLLISION_AVOIDANCE_SIM_EXPORT_ENV=1
## --------------------------------------------

## ---- ROS2 CONTROLLER CONTAINER SERVICE ----
CONTAINER_ROS2_CONTROLLER_NAME=sitl-ros2-ct
CONTAINER_ROS2_CONTROLLER_HOSTNAME=${CONTAINER_ROS2_CONTROLLER_NAME}
CONTAINER_ROS2_CONTROLLER_IMAGE=jociiiii/ros2:dev-humble-cu1180d-ct-v1.0
CONTROLLOER_SOURCE_CODE_DIR=<YOUR_DIR_TO_CONTROLLOER_SOURCE_CODE>

ROS2_CONTROLLER_DEBUG_MODE=0
ROS2_CONTROLLER_REBUILD_MODE=0
ROS2_CONTROLLER_SIM_EXPORT_ENV=1
## --------------------------------------------
